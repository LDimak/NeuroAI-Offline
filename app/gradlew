#!/usr/bin/env sh

##############################################################################
##
##  Gradle start-up script for UN*X
##
##############################################################################

# Locate the gradle wrapper jar
SCRIPT_DIR="$(cd "$(dirname "$0")" && pwd)"
CLASSPATH="$SCRIPT_DIR/gradle/wrapper/gradle-wrapper.jar"

# Execute the wrapper
exec java -Dorg.gradle.appname="app" -classpath "$CLASSPATH" org.gradle.wrapper.GradleWrapperMain "$@"
